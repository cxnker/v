local cars = {
    {"CyberTruck"},
    {"Roadster"},
    {"Bugatti"},
    {"SuperCar"},
    {"Lamboego"},
    {"CopLamborgini"},
	{"Ferrari"},
	{"Formula1"},
    {"Buggy"},
	{"GoKartVPass"},
	{"BatMobile"},
